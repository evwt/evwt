{
  "name": "evwt",
  "version": "0.0.3",
  "description": "Electron Vue Window Toolkit",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evwt/evwt.git"
  },
  "bugs": {
    "url": "https://github.com/evwt/evwt/issues"
  },
  "homepage": "https://evwt.net",
  "author": {
    "name": "John Susek",
    "email": "john@johnsolo.net"
  },
  "scripts": {
    "watch": "nodemon --ignore dist/ --exec npm run build -e scss,js,vue",
    "build": "npm run build:umd & npm run build:es",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/evwt.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/evwt.esm.js"
  },
  "main": "index.js",
  "module": "dist/evwt.esm.js",
  "dependencies": {
    "core-js": "3.6.5",
    "lodash": "4.17.19",
    "vue": "2.6.11"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "3.1.1",
    "@rollup/plugin-buble": "0.21.3",
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-airbnb": "5.0.2",
    "babel-eslint": "10.1.0",
    "eslint": "6.7.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-vue": "6.2.2",
    "nodemon": "2.0.4",
    "rollup": "2.23.0",
    "rollup-plugin-css-only": "2.1.0",
    "rollup-plugin-scss": "2.5.0",
    "rollup-plugin-vue": "5.1.9",
    "sass": "1.26.5",
    "sass-loader": "8.0.2",
    "vue-svg-loader": "0.16.0",
    "vue-template-compiler": "2.6.11"
  },
  "peerDependencies": {
    "electron": "*"
  },
  "keywords": [
    "electron",
    "vue",
    "electron-vue",
    "components",
    "toolkit",
    "menu",
    "native"
  ]
}
